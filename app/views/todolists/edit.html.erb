<h1>編集画面</h1>
<%= form_for(@list, url:"/todolists/#{@list.id}") do |f| %>
	<h4>タイトル</h4>
	<%= f.text_field :title %>
	<h4>本文</h4>
	<%= f.text_area :body %>
	<h4>画像</h4>
	<%= attachment_image_tag @list, :image, :fill, 200, 200, format: 'jpeg' %>
	<%= f.attachment_field :image %>
	<%= f.submit '保存' %>
<% end %>